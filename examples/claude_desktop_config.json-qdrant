{
  "mcpServers": {
    "qdrant": {
      "command": "uvx",
      "args": [
        "mcp-openapi-proxy"
      ],
      "env": {
        "OPENAPI_SPEC_URL": "https://raw.githubusercontent.com/qdrant/qdrant/master/docs/redoc/master/openapi.json",
        "SERVER_URL_OVERRIDE": "https://localhost:6333",
        "TOOL_WHITELIST": "/collections",
        "API_KEY": "your_qdrant_token_here"
      }
    }
  }
}
