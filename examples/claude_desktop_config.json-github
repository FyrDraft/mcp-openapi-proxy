{
    "mcpServers": {
        "git": {
            "command": "uvx",
            "args": ["mcp-openapi-proxy"],
            "env": {
                "OPENAPI_SPEC_URL": "https://api.apis.guru/v2/specs/github.com/github.ae/1.1.4/openapi.json",
                "TOOL_WHITELIST": "/repos",
                "API_KEY": "your-github-token-here"
            }
        }
    }
}
