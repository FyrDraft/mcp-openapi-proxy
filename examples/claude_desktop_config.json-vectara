{
  "mcpServers": {
    "vectara": {
      "command": "uvx",
      "args": [
        "mcp-openapi-proxy"
      ],
      "env": {
        "OPENAPI_SPEC_URL": "https://docs.vectara.com/vectara-oas.yaml",
        "TOOL_WHITELIST": "/v1/search",
        "API_KEY": "your_vectara_token_here",
        "TOOL_NAME_PREFIX": "vectara"
      }
    }
  }
}
